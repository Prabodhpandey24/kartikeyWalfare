<section id="objective" class="py-5">
	<div class="sliderHeading d-flex justify-content-center ">
		<h4 class="sliderHeadingText">Our Objective</h4>
	</div>
	<div class="ojectiveCard py-5">
		<div class="container">
			<div class="row">
				{{#each objectiveCardData}}
				<div class="col-sm-3">
					<div class="objectiveCard mt-3">
						<div class="alignJustify">
							<img src={{this.imgUrl}} alt="" class="objectiveImg " />
						</div>
						<h4 class="alignJustify">{{name}}</h4>
					</div>
				</div>
				{{/each}}
			</div>
		</div>

	</div>
</section>